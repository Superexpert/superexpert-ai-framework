{
  "name": "@superexpert-ai/framework",
  "version": "1.0.9",
  "description": "Platform classes for creating Superexpert.AI plugins",
  "keywords": [
    "superexpert-ai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/superexpert/superexpert-ai-framework"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "author": "Stephen Walther",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.MD"
  ],
  "scripts": {
    "build": "rimraf dist && tsup",
    "prepublishOnly": "npm run build",
    "release:patch": "npm run build && npm version patch -m \"chore(release): %s\" && npm publish",
    "release:minor": "npm run build && npm version minor -m \"feat!: %s\" && npm publish",
    "release:major": "npm run build && npm version major -m \"feat!: %s\" && npm publish",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "peerDependencies": {
    "@prisma/client": "^6.5.0"
  },
  "dependencies": {
    "@prisma/client": "^6.5.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^19.0.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "user": "^0.0.0"
  },
  "devDependencies": {
    "@prisma/client": "^6.5.0",
    "@types/node": "^22.13.11",
    "@types/react": "^19",
    "prisma": "^6.5.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2"
  }
}
